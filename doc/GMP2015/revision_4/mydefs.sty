\newlength\myMinHeight
\setlength\myMinHeight{0pt}
\newsavebox\mySubfigsTempSaveBox

\newcommand\ClearMyMinHeight{\setlength\myMinHeight{0pt}}
\newcommand\SetMyMinHeight[2]{%width, filename
    \sbox\mySubfigsTempSaveBox{\includegraphics[width=#1\linewidth]{#2}}%
    \ifdim\ht\mySubfigsTempSaveBox<\myMinHeight
        \setlength\myMinHeight{\ht\mySubfigsTempSaveBox}\fi%
    \ifdim\myMinHeight=0pt
        \setlength\myMinHeight{\ht\mySubfigsTempSaveBox}\fi%
}



